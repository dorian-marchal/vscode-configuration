{
  "workbench.colorCustomizations": {
    "statusBar.background": "#bbb",
    "statusBar.foreground": "#333",
    "statusBar.noFolderForeground": "#fff"
  },
  "workbench.settings.editor": "json",
  "editor.formatOnSave": true,
  "editor.autoIndent": "brackets",
  "workbench.startupEditor": "newUntitledFile",
  "editor.multiCursorModifier": "ctrlCmd",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "editor.renderControlCharacters": true,
  "explorer.confirmDragAndDrop": false,
  "twigcs.executablePath": "/home/dorian/.config/composer/vendor/bin/twigcs",
  "editor.dragAndDrop": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/vendor": true,
    "**/dist": true,
    "**/.git": true,
    "**/.elixir_ls": true,
  },
  "files.exclude": {
    "**/.git": false
  },
  "editor.minimap.enabled": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.advanced.messages": {
    "suppressGitVersionWarning": true,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true
  },
  "editor.rulers": [
    80
  ],
  "sort-imports.on-save": false,
  "window.zoomLevel": 0,
  "todohighlight.keywords": [
    "@FIXME",
    "@TODO",
    "@tag :only",
  ],
  "extensions.ignoreRecommendations": false,
  "workbench.editor.tabSizing": "shrink",
  "gitlens.keymap": "alternate",
  "workbench.commandPalette.history": 500,
  "emmet.includeLanguages": {
    "twig": "html",
    "javascript": "javascriptreact"
  },
  "emojisense.unicodeCompletionsEnabled": false,
  "emojisense.showOnColon": false,
  "files.associations": {
    ".my-bashrc": "shellscript",
    "*.css": "css",
    ".env": "shellscript",
    "*.env": "shellscript",
    "*.phpt": "toml",
    ".gitalias": "shellscript"
  },
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "window.enableMenuBarMnemonics": false,
  // Avoid . from triggering autocomplete (e.g. ..).
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.snippetSuggestions": "top",
  // Ignore whitespace changes and file mv.
  "gitlens.advanced.blame.customArguments": [
    "-w",
    "-M"
  ],
  "[php]": {
    "editor.formatOnSave": false,
    // Removes $.
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.defaultFormatter": "persoderlind.vscode-phpcbf"
  },
  "[elixir]": {
    "editor.wordBasedSuggestions": true,
    // Enable this if you can't get used to <tab> as completion char.
    //   "editor.acceptSuggestionOnEnter": "on",
  },
  "[javascript]": {
    // Removes $.
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[twig]": {
    "editor.formatOnSave": false
  },
  "[shellscript]": {
    // Removes $.
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?"
  },
  "[scss]": {
    // Removes $.
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?"
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.tabSize": 2
  },
  "[toml]": {
    "files.trimTrailingWhitespace": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "gitlens.views.compare.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.currentLine.scrollable": false,
  "gitlens.blame.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.recentChanges.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.views.repositories.enabled": false,
  "gitlens.views.fileHistory.avatars": false,
  "gitlens.views.lineHistory.avatars": false,
  "gitlens.views.compare.files.compact": false,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.inputValidationSubjectLength": 50,
  "git.showPushSuccessNotification": true,
  "gitlens.advanced.telemetry.enabled": false,
  "zenMode.fullScreen": true,
  "zenMode.centerLayout": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "versionlens.showVersionLensesAtStartup": false,
  "diffEditor.renderSideBySide": false,
  "diffEditor.renderIndicators": false,
  "editor.renderWhitespace": "all",
  "emmet.triggerExpansionOnTab": true,
  "editor.formatOnSaveTimeout": 10000,
  "bracket-pair-colorizer-2.colors": [
    "#bbb", // gray
    "#e4d98b", // yellow
    "#e4adad", // red
    "#a8b2eb", // blue
    "#9fd89f" // green
  ],
  "editor.emptySelectionClipboard": false,
  "open-in-browser.default": "google-chrome",
  "window.menuBarVisibility": "hidden",
  "phpcs.showWarnings": false,
  "markdown.extension.toc.githubCompatibility": true,
  "markdown.extension.toc.levels": "1..6",
  "markdown.extension.toc.unorderedList.marker": "-",
  "markdown.extension.toc.orderedList": false,
  "markdown.extension.toc.plaintext": false,
  "markdown.extension.toc.updateOnSave": true,
  "markdown.extension.toc.downcaseLink": true,
  "markdown.extension.preview.autoShowPreviewToSide": false,
  "markdown.extension.orderedList.marker": "ordered",
  "markdown.extension.italic.indicator": "*",
  "markdown.extension.tableFormatter.normalizeIndentation": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "markdown.extension.toc.omittedFromToc": {
    "docs/SEO.md": [
      "# Documentation de la configuration SEO",
      "## Sommaire",
    ],
  },
  "git-graph.defaultFileViewType": "File List",
  "gitlens.views.compare.files.layout": "list",
  "files.defaultLanguage": "markdown",
  "breadcrumbs.enabled": true,
  "html.mirrorCursorOnMatchingTag": true,
  "editor.suggest.insertMode": "replace",
  "elixirLS.suggestSpecs": false,
  "editor.wordBasedSuggestions": false,
  "vetur.format.defaultFormatter.html": "prettier",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "togglequotes.chars": [
    "\"",
    "'",
    "`"
  ],
  "auto-comment-next-line.list": [
    {
      "char": "//",
      "languages": [
        "php",
        "javascript",
        "jsonc"
      ]
    },
    {
      "char": "#",
      "languages": [
        "python",
        "elixir"
      ]
    }
  ]
}
